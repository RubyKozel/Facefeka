{
  "name": "facefeka",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha --timeout 10000 server/**/*.test.js",
    "test-watch": "nodemon --exec \"npm test\"",
    "build": "webpack -p --progress --colors --devtool source-map --config webpack.config.js",
    "start-game-server": "php game/server/server.php",
    "start-node-server-production": "export NODE_ENV=production || set \"NODE_ENV=production\" && npm run build && node website/server/server.js",
    "start-node-server-development": "export NODE_ENV=development || set \"NODE_ENV=development\" && npm run build && node website/server/server.js",
    "start-development": "concurrently \"npm run start-game-server\" \"npm run start-node-server-development\"",
    "start": "concurrently \"npm run start-game-server\" \"npm run start-node-server-production\""
  },
